{
  "name": "ummgoban-web-view",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev:client": "pnpm --filter @ummgoban/client dev:host",
    "dev:admin": "pnpm --filter @ummgoban/admin dev",
    "dev:ui": "pnpm --filter @packages/ui dev",
    "dev:shared": "pnpm --filter @packages/shared dev",
    "dev:client-all": "concurrently --colors \"pnpm dev:ui\" \"pnpm dev:shared\" \"pnpm dev:client\"",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "preview": "pnpm --filter @ummgoban/client preview"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vitest": "^3.2.4"
  }
}
