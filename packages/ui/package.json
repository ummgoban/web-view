{
  "name": "@packages/ui",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist && pnpm rollup -c rollup.config.cjs --bundleConfigAsCjs",
    "dev": "rm -rf dist && pnpm rollup -c rollup.config.cjs --bundleConfigAsCjs -w",
    "storybook": "storybook dev -p 6006",
    "format": "prettier --write .",
    "build:storybook": "rm -rf storybook-static && storybook build"
  },
  "dependencies": {
    "@shadcn/ui": "^0.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "reset-css": "^5.0.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.7",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/builder-vite": "^8.2.10",
    "@storybook/react": "^8.2.10",
    "@storybook/react-vite": "^8.2.10",
    "@storybook/test": "^8.6.14",
    "@types/node": "^22.17.1",
    "@types/react": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "storybook": "^8.2.10",
    "tslib": "^2.8.1",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
