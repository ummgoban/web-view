{
  "name": "@ummgoban/client",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc -b && vite build",
    "build:prod": "pnpm run build",
    "build:dev": "NODE_ENV=development vite build --mode development",
    "lint": "eslint .",
    "preview": "NODE_ENV=local vite preview --port 8080",
    "preview:host": "NODE_ENV=local vite preview --host",
    "deploy:prod": "sh ./.script/deploy.sh prod",
    "deploy:dev": "sh ./.script/deploy.sh dev"
  },
  "dependencies": {
    "@packages/shared": "workspace:*",
    "@packages/ui": "workspace:*",
    "@tanstack/react-query": "^5.84.2",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.0",
    "reset-css": "^5.0.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "vite-plugin-svgr": "^4.3.0"
  }
}
